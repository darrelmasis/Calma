{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server/src/api/booking-send-mail.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/booking-send-mail",
      "dest": "/server/src/api/booking-send-mail.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
