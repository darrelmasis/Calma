{
  "version": 2,
  "routes": [
    {
      "src": "/manifest.webmanifest",
      "dest": "client/manifest.webmanifest"
    },
    {
      "src": "/favicon.ico",
      "dest": "client/favicon.ico"
    },
    {
      "src": "/api/booking/send-mail",
      "methods": ["POST"],
      "dest": "/server/src/api/booking-send-mail.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "client/assets/$1"
    },
    {
      "src": "/(.*\\.(svg|css|js|png|jpg|webp|ttf|woff2|ogg|mp3))",
      "dest": "client/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/robots.txt",
      "dest": "client/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "client/sitemap.xml"
    },
    {
      "src": "/humans.txt",
      "dest": "client/humans.txt"
    },
    {
      "src": "/(.*)",
      "dest": "client/index.html",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    }
  ]
}
